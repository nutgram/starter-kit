{
  "name": "nutgram/starter-kit",
  "description": "Starter kit for Nutgram",
  "keywords": [
    "nutgram",
    "telegram",
    "bot"
  ],
  "homepage": "https://github.com/Nutgram/starter-kit",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "nutgram/nutgram": "^4.40.0",
    "vlucas/phpdotenv": "^5.6",
    "symfony/cache": "^8.0"
  },
  "require-dev": {
    "pestphp/pest": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Nutgram\\StarterKit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nutgram\\StarterKit\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
